{
  "name": "speck",
  "description": "Specification and planning workflow framework for Claude Code",
  "version": "1.7.6",
  "author": {
    "name": "Nathan Prabst",
    "email": "nathan@example.com"
  },
  "homepage": "https://github.com/nprbst/speck",
  "repository": "https://github.com/nprbst/speck",
  "license": "MIT",
  "keywords": [
    "specification",
    "planning",
    "workflow",
    "feature-management",
    "development-tools"
  ],
  "hooks": {
    "PreToolUse": [
      {
        "matcher": "Bash",
        "hooks": [
          {
            "type": "command",
            "command": "bun ${CLAUDE_PLUGIN_ROOT}/dist/speck-hook.js --hook"
          }
        ]
      }
    ],
    "UserPromptSubmit": [
      {
        "matcher": ".*",
        "hooks": [
          {
            "type": "command",
            "command": "bun ${CLAUDE_PLUGIN_ROOT}/dist/pre-prompt-submit-hook.js"
          }
        ]
      }
    ]
  }
}
